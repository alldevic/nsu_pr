INCLUDE "comp2.inc";
SUBDESIGN comp4
(
	a[3..0], b[3..0]	: INPUT ;
	G, L				: OUTPUT;
)
VARIABLE
	cmp2[2..0]			: comp2;
BEGIN
	cmp2[0].(a[1..0], b[1..0]) = (a[1..0], b[1..0]);
	cmp2[1].(a[1..0], b[1..0]) = (a[3..2], b[3..2]);
	cmp2[2].(a[1..0], b[1..0]) = (cmp2[1..0].G, cmp2[1..0].L);

	(G, L) = cmp2[2].(G, L);
END;

